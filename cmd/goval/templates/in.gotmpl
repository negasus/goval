{ // {{ .field }}
    if !slices.Contains({{ .sliceType }}{{ .var }}, model.{{ .field }}) {
        errors["{{ .name }}"] = append(errors["{{ .name }}"], goval.Error{
            Type: goval.{{ .errorType }},
            Values: map[string]any{
                "field": "{{ .name }}",{{ range $key, $value := .meta }}{{"\n"}}"{{ $key }}": {{ $value }},{{ end }}
            },
        })
    }
}
